@*use site css elements*@
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] -      BloodSugar</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BloodSugar.styles.css" asp-append-version="true" />
</head>
<body>
    <header class="navbar navbar-expand navbar-dark bg-dark">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">BloodSugar</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <nav>

                
                    <a class="x" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    
                    <a class="x" asp-area="" asp-controller="UserPage" asp-action="UserPageList">User list</a>
                    <a class="x" asp-area="" asp-controller="UserPage" asp-action="AddUser">Add user</a>
                

                    @*
                    <a class="x" asp-area="" asp-controller="MeasurePage" asp-action="AddBSMeasure">Add BS</a>
                    <a class="x" asp-area="" asp-controller="MeasurePage" asp-action="AddInsulinMeasure">Add Insulin</a>
                    <a class="x" asp-area="" asp-controller="MeasurePage" asp-action="AddCarbMeasure">Add Carb</a>
                    *@
        
                    <a class="x" asp-area="" asp-controller="MeasurePage" asp-action="BSMeasurePageList">Blood Sugar</a>
                    <a class="x" asp-area="" asp-controller="MeasurePage" asp-action="InsulinMeasurePageList">Insulin</a>
                    <a class="x" asp-area="" asp-controller="MeasurePage" asp-action="CarbMeasurePageList">Carb</a>

                    
                    
                




                <div class="animation start-home"></div>
        </nav>
     </header>
        
        

        
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
